{
  "name": "ql_ts",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "main": "dist/src/Index.js",
  "targets": {
    "main": false
  },
  "license": "GPL-3.0-or-later",
  "scripts": {
    "=====dev=====": "",
    "dev:cmd": "cross-env NODE_ENV=dev DEBUG=App:*,Package:* DEBUG_HIDE_DATE=true node dist/src/Cmd.js",
    "dev:service": "cross-env NODE_ENV=dev DEBUG=App:*,Package:* DEBUG_HIDE_DATE=true nodemon dist/src/Service.js",
    "=====build=====": "",
    "build:all": "rm -rf dist && npm run build:all:lazy",
    "build:all:lazy": "npm run build:web && npm run build:ts",
    "build:web": "parcel build ./index.html --no-cache --dist-dir dist/web --public-url ./",
    "build:ts": "tsc",
    "=====watch=====": "",
    "watch:web": "rm -rf .parcel-cache dist/web && parcel --dist-dir dist/web ./index.html",
    "watch:ts": "tsc -w",
    "=====test=====": "",
    "test": "cross-env NODE_ENV=dev mocha dist/test/index.test.js --bail --timeout 99999999",
    "coverage": "cross-env NODE_ENV=dev nyc --clean --reporter=html --reporter=text-summary npm run test && open-cli ./coverage/index.html",
    "=====npm=====": "",
    "prepub": "npm i && npm run build:all",
    "pub": "npm publish --access=public",
    "=====其他=====": "",
    "format:all": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@lsby/ts_lambda_type": "^0.0.10",
    "@lsby/ts_pattern": "^0.0.15",
    "@lsby/ts_type_check": "^0.0.10",
    "@lsby/ts_type_fun": "^0.0.9",
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "element-plus": "^2.2.11",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "jquery": "^3.6.0",
    "kysely": "^0.17.1",
    "layer-src": "^3.5.1",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "mysql-import": "^5.0.21",
    "mysql2": "^2.3.3",
    "ramda": "^0.28.0",
    "session-file-store": "^1.5.0",
    "supports-color": "^9.2.2",
    "uuid": "^8.3.2",
    "vue": "^3.2.35",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@parcel/transformer-vue": "^2.6.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.5",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.0",
    "@types/mysql": "^2.15.21",
    "@types/ramda": "^0.28.14",
    "@types/session-file-store": "^1.2.2",
    "@types/uuid": "^8.3.1",
    "husky": "^7.0.4",
    "knex": "^1.0.4",
    "mocha": "^9.2.2",
    "mysqldump": "^3.2.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "open-cli": "^7.0.1",
    "parcel": "^2.6.2",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "process": "^0.11.10",
    "typescript": "^4.7.3"
  }
}
