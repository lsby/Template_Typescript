{
  "name": "ql_ts",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "main": "dist/src/Index.js",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "=====dev=====": "",
    "dev:app": "cross-env NODE_ENV=dev DEBUG=App:*,Package:* DEBUG_HIDE_DATE=true DEBUG_DEPTH=999 electron ./dist/src/Electron.js",
    "dev:cmd": "cross-env NODE_ENV=dev DEBUG=App:*,Package:* DEBUG_HIDE_DATE=true DEBUG_DEPTH=999 node dist/src/Cmd.js",
    "dev:service": "cross-env NODE_ENV=dev DEBUG=App:*,Package:* DEBUG_HIDE_DATE=true DEBUG_DEPTH=999 nodemon dist/src/Service.js",
    "=====build=====": "",
    "build:all": "rm -rf dist && npm run build:all:lazy",
    "build:all:lazy": "npm run build:ts && npm run build:web",
    "build:web": "webpack --config ./webpack/webpack.config.prod.js",
    "build:web:analyzer": "cross-env use_analyzer=true npm run build:web",
    "build:ts": "tsc",
    "=====watch=====": "",
    "watch:web": "webpack --config ./webpack/webpack.config.dev.js",
    "watch:ts": "tsc -w",
    "=====test=====": "",
    "test": "cross-env NODE_ENV=dev mocha dist/test/index.test.js --bail --timeout 99999999",
    "coverage": "cross-env NODE_ENV=dev nyc --clean --reporter=html --reporter=text-summary npm run test && open-cli ./coverage/index.html",
    "=====npm=====": "",
    "prepub": "npm i && npm run build:all",
    "pub": "npm publish --access=public",
    "=====其他=====": "",
    "pack": "npm run build:all && electron-builder",
    "format:all": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "build": {
    "appId": "ql_ts.app",
    "win": {
      "target": "portable"
    },
    "files": [
      "./.env/**/*",
      "./dist/**/*",
      "./node_modules/**/*"
    ],
    "directories": {
      "output": "./pack"
    },
    "extraMetadata": {
      "main": "dist/src/Electron.js"
    }
  },
  "dependencies": {
    "@lsby/ts_type_check": "^0.0.10",
    "@lsby/ts_type_fun": "^0.0.12",
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "element-plus": "^2.2.11",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "kysely": "^0.17.1",
    "lodash": "^4.17.21",
    "make-dir": "^3.1.0",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "mysql-import": "^5.0.21",
    "mysql2": "^2.3.3",
    "ramda": "^0.28.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "supports-color": "^9.2.2",
    "uuid": "^8.3.2",
    "vue": "^3.2.35",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.5",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.0",
    "@types/mysql": "^2.15.21",
    "@types/ramda": "^0.28.14",
    "@types/session-file-store": "^1.2.2",
    "@types/uuid": "^8.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "electron": "^20.3.3",
    "electron-builder": "^23.3.3",
    "event-hooks-webpack-plugin": "^2.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "knex": "^1.0.4",
    "mocha": "^9.2.2",
    "mysqldump": "^3.2.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "open-cli": "^7.0.1",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vue-loader": "^17.0.0",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  }
}
